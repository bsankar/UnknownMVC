@model List<UnknownMVC.Models.Task>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/_LayoutOne.cshtml";
}


<h2>Index</h2>
@if(Model==null || Model.Count==0)
{
    <h1>No records found</h1>
 }
else
{
foreach(UnknownMVC.Models.Task tsk in Model)
{
    <h2>@tsk.TaskDescription</h2><br/>
    
}
}